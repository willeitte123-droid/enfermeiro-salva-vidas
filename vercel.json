{
  "rewrites": [
    {
      "source": "/((?!assets/|images/|sw.js|manifest.json|logo.svg|robots.txt|offline.html|favicon.ico).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}